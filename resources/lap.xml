<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
	
	<bean class ="com.xworkz.spring.Laptop" id = "laptop">
	   <constructor-arg  value="hp"></constructor-arg>
	    <constructor-arg  value="50000.00"></constructor-arg>
		<constructor-arg  value="4"></constructor-arg>
		<constructor-arg  value="Black"></constructor-arg>
	
	</bean>	
	
	<bean class ="com.xworkz.spring.Laptop" id="hp">
	    <constructor-arg value = "Dell"></constructor-arg>
		<constructor-arg value = "50000.00"></constructor-arg>
	
	</bean>
		
	<bean class ="com.xworkz.spring.Laptop" id="laptopAcer">
	    <constructor-arg value = "acer"></constructor-arg>
		<constructor-arg value = "50000.00"></constructor-arg>
	</bean>
	
</beans>