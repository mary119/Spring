<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
	
	<bean class="com.xworkz.init.resourceSpring.RocketDTO" id="rocketDTO" >
	<property name="id" value="2"></property>
	<property name="country" value="Canada"></property>
	<property  name="launchDate" value="1993.09.30"></property>
	<property  name="cost" value="1300000.00"></property>
	</bean>
	
	
	<bean class = "com.xworkz.init.resourceSpring.RocketDAO" id = "rocketDAO" init-method="initResource" destroy-method="closeResource">
	<constructor-arg value="jdbc:mysql://localhost:3306"></constructor-arg>
	<constructor-arg value="com.mysql.cj.jdbc.Driver"></constructor-arg>
    <property name="userName" value="root"></property>
	<property  name="password" value="rootmary"></property>
	</bean>
	
	
	
	
	
	
</beans>